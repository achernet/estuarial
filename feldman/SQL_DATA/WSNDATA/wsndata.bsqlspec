select item, 
    m.seccode, 
    d.year_, 
    d.seq, 
    d.date_ as ddate, 
    d.value_, 
    f.date_  as fdate,
    freq
from wsndata d
  join secmapx m on m.ventype = 10 and m.vencode = d.code and rank = 1
  left outer join wsfye f on f.code = d.code and f.year_ = d.year_
---
seccode, item, freq
---
fdate
